	<queryString language="SQL">
		<![CDATA[SELECT 
    t.id_transaksi,
    t.nama_pelanggan,
    t.tgl,
    p.nama_produk,
    d.jumlah_beli,
    d.subtotal,
    t.tunai AS bayar,
    t.kembalian,
    u.nama_user AS nama_kasir
FROM 
    transaksi t
JOIN 
    detail_transaksi d ON t.id_transaksi = d.id_transaksi
JOIN 
    produk p ON d.id_produk = p.id_produk
JOIN 
    user u ON t.id_user = u.id_user
ORDER BY 
    t.id_transaksi;
]]>
	</queryString>
