	<queryString language="SQL">
		<![CDATA[SELECT 
    t.id_transaksi,
    u.nama_user AS nama_kasir,
    t.nama_pelanggan,
    t.tgl,
    p.nama_produk,
    dt.jumlah_beli,
    p.harga,
    dt.subtotal,
    t.total_bayar,
    t.tunai,
    t.kembalian
FROM 
    transaksi t
JOIN 
    user u ON t.id_user = u.id_user
JOIN 
    detail_transaksi dt ON t.id_transaksi = dt.id_transaksi
JOIN 
    produk p ON dt.id_produk = p.id_produk
WHERE 
    t.id_transaksi]]>
	</queryString>
